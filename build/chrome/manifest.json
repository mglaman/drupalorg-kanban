{
  "manifest_version": 2,
  "name": "drupalorg-kanban",
  "homepage_url": "https://github.com/mglaman/drupalorg-kanban",
  "version": "1.0.0",
  "description": "An extension for Drupal.org to embed ContribKanban.com",
  "author": "Matt Glaman",
  "icons": { "128": "icon.png" },
  "content_scripts": [
    {
      "matches": [
        "*://*.drupal.org/*",
        "*://*.devdrupal.org/*"
      ],
      "js": ["drupalorg-kanban.js"]
    }
  ],
  "permissions": [
    "*://*.drupal.org/*",
    "*://*.devdrupal.org/*"
  ]
}
